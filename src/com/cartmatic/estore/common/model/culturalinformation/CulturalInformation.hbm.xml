<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.cartmatic.estore.common.model.culturalinformation.CulturalInformation"
		table="cultural_information" select-before-update="false"
		dynamic-update="false" dynamic-insert="false" optimistic-lock="version">
		<id name="culturalInformationId" column="culturalInformationId"
			type="java.lang.Integer" length="10">
			<generator class="native" />
		</id>
		<property name="title" type="java.lang.String" update="true"
			insert="true" access="property" column="title" length="128" not-null="true"
			unique="false" />

		<property name="textIntroduction" type="java.lang.String"
			update="true" insert="true" access="property" column="textIntroduction"
			length="65535" not-null="false" unique="false" />

		<property name="recommendArrayId" type="java.lang.String"
			update="true" insert="true" access="property" column="recommendArrayId"
			length="30" not-null="false" unique="false" />


		<property name="commentNumber" type="java.lang.Integer"
			update="true" insert="true" access="property" column="commentNumber"
			length="10" not-null="false" unique="false" />
			
		<property name="releaseTime" type="java.util.Date" update="true"
			insert="true" access="property" column="releaseTime" length="128"
			not-null="false" unique="false" />
			
		<property name="readNumber" type="java.lang.Integer" update="true"
			insert="true" access="property" column="readNumber" length="10"
			not-null="false" unique="false" />

		<property name="state" type="java.lang.Integer" update="true"
			insert="true" access="property" column="state" length="10" not-null="false"
			unique="false" />

		<property name="writer" type="java.lang.String" update="true"
			insert="true" access="property" column="writer" length="20" not-null="false"
			unique="false" />
		<property name="type" type="java.lang.Integer" update="true"
			insert="true" access="property" column="type" length="10" not-null="true"
			unique="false" />
		<property name="logoImg" type="java.lang.String" update="true"
			insert="true" access="property" column="logoImg" length="128"
			not-null="false" unique="false" />
		<property name="imgOne" type="java.lang.String" update="true"
			insert="true" access="property" column="imgOne" length="128"
			not-null="false" unique="false" />
		<property name="imgTwo" type="java.lang.String" update="true"
			insert="true" access="property" column="imgTwo" length="128"
			not-null="false" unique="false" />
		<property name="brandId" type="java.lang.Integer" update="true"
			insert="true" access="property" column="brandId" length="10"
			not-null="false" unique="false" />
		<property name="sortOrder" type="java.lang.Integer" update="true"
			insert="true" access="property" column="sortOrder" length="10"
			not-null="false" unique="false" />
		<property name="createTime" type="java.util.Date" update="true"
			insert="true" access="property" column="createTime" length="20"
			not-null="false" unique="false" />
		<property name="videoAddress" type="java.lang.String" update="true"
			insert="true" access="property" column="videoAddress" length="20"
			not-null="false" unique="false" />
		<property name="backOne" type="java.lang.String" update="true"
			insert="true" access="property" column="backOne" length="20"
			not-null="false" unique="false" />
		<property name="backTwo" type="java.lang.String" update="true"
			insert="true" access="property" column="backTwo" length="20"
			not-null="false" unique="false" />
		<property name="metaKeywork" type="java.lang.String" update="true"
			insert="true" access="property" column="metaKeywork" length="20"
			not-null="false" unique="false" />
			
			<!-- exported bi-directional one-to-many association to monthlyCultural -->
		<set name="monthlyCultural" table="monthly_cultural" lazy="false" inverse="true" cascade="save-update,persist" order-by="monthlyCulturalId desc" >
			<key>
				<column name="culturalInformationId" />
			</key>
			<one-to-many class="com.cartmatic.estore.common.model.monthlycultural.MonthlyCultural" />
		</set>
	</class>
</hibernate-mapping>
